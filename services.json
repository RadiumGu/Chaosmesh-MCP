{
    "mcp_configuration": {
        "chaosmesh-mcp": {
            "autoApprove": [],
            "disabled": false,
            "command": "uv",
            "args": [
                "--directory",
                "/home/ec2-user/mcp-servers/chaosmesh-mcp",
                "run",
                "python",
                "server.py",
                "--transport",
                "stdio"
            ],
            "env": {
                "AWS_PROFILE": "default",
                "AWS_REGION": "us-east-2"
            },
            "transportType": "stdio"
        }
    },
    "services": [
        {
            "name": "adservice",
            "namespace": "default",
            "type": "pod",
            "labels": [
                "app=adservice"
            ],
            "containers": [
                {
                    "name": "server"
                }
            ]
        },
        {
            "name": "cartservice",
            "namespace": "default",
